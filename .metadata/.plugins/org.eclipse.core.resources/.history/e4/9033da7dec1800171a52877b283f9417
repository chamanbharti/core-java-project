package com.javaconceptodtheday;

class Outer{
	class Inner{
		public void m1(){
			System.out.println("Inner class method");
		}
	}
}
public class Demo{
	public static void main(String[] args){
		//Step-I
		Outer o = new Outer();
		Outer.Inner i = o.new Inner();
		i.m1();

		//Step-II
		/*Outer o = new Outer();
		Outer.Inner i = new Outer().new Inner();
		i.m1();*/

		//Step-III
		//new Outer().new Inner().m1();
	}
}
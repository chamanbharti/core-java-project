package constructor;

public class CopyConstructor2 {
	
	 	int id;  
	    String name; 
	    
	    CopyConstructor2(int i,String n){  
		    id = i;  
		    name = n;  
	    }  
	      
	    CopyConstructor2(CopyConstructor2 s){  
		    id = s.id;  
		    name =s.name;  
	    }  
	    
	    void display(){
	    		System.out.println(id+" "+name);
	    	}  
	   
	    public static void main(String args[]){  
	    	
	    	CopyConstructor2 s1 = new CopyConstructor2(111,"Karan");  
	    	CopyConstructor2 s2 = new CopyConstructor2(s1); 
	    	
		    s1.display();  
		    s2.display();  
	   }  

}
